    "url": "https://github.com/Silvershot335/SilverBot/issues"
  },
  "homepage": "https://github.com/Silvershot335/SilverBot#readme",
  "dependencies": {
    "@types/node-fetch": "^2.5.4",
    "discord.js": "^11.5.1",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "concurrently": "^5.0.0",
    "pre-commit": "^1.2.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  }
