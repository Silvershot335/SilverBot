{
  "name": "silverbot",
  "version": "1.0.0",
  "description": "Wavy, Baby",
  "main": "lib",
  "scripts": {
    "start": "npm run compile && concurrently \"npm:run-node\" \"npm:compile-watch\"",
    "run-node": "nodemon -r dotenv/config .",
    "compile": "tsc",
    "compile-watch": "tsc --watch",
    "prod": "npm run compile && npm run start-batch",
    "start-batch": "node -r dotenv/config . production",
    "lint": "tslint -p src"
  },
  "pre-commit": [
    "lint",
    "compile"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Silvershot335/SilverBot.git"
  },
  "author": "SilverShot",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Silvershot335/SilverBot/issues"
  },
  "homepage": "https://github.com/Silvershot335/SilverBot#readme",
  "dependencies": {
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.1",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.24",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "@types/node-fetch": "^2.5.4",
    "concurrently": "^5.0.0",
    "pre-commit": "^1.2.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  }
}
